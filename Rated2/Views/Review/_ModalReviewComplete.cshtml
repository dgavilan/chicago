<div id="modalReviewComplete" class="modal fade" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">Review Complete</h4>
            </div>
            <div class="modal-body">
                
                Rating:
                <select id="DetailRating" name="DetailRating">
                    <option value="5">5</option>
                    <option value="4">4</option>
                    <option value="3">3</option>
                    <option value="2">2</option>
                    <option value="1">1</option>
                </select>

                <br />
                <br />

                Comments:
                <textarea name="ReviewerComments" id="ReviewerComments" class="form-control"></textarea>


            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-link" data-dismiss="modal">Cancel</button>
                <button type="button" class="btn btn-primary" onclick="Project.SubmitReview()">Submit Review</button>

            </div>
        </div>
    </div>
</div>